@model IEnumerable<EFcodefirst.Models.Student>


<h2>Index</h2>

@Html.ActionLink("Create new one","Create","Home")
<table class="table" border="1">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Age)
        </th>
        
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
           
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Age)
            </td>
           
            <td>
                @Html.ActionLink("Edit", "Edit","Home", new { id = item.Id }) |

                @Html.ActionLink("Delete", "Delete","Home", new { id = item.Id }, new { onclick = "return confirm('Are sure wants to delete?');"})
            </td>

        </tr>
    }

</table>
